    "@iconify-json/ri": "^1.1.2",
    "@iconify-json/twemoji": "^1.1.4",
    "@pureadmin/theme": "^1.1.0",
    "@types/element-resize-detector": "1.1.3",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.182",
